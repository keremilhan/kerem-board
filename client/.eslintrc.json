{
    "extends": ["react-app"],
    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "off",
        "react-hooks/exhaustive-deps": "off",
        "no-unused-expressions": "off",
        "no-sequences": "off",
        "new-parens": "off",
        "no-mixed-operators": "off",
        "no-use-before-define": "off",
        "no-useless-concat": "off",
        "no-self-compare": "off",
        "no-func-assign": "off",
        "no-undef": "off",
        "no-restricted-globals": "off",
        "no-useless-escape": "off",
        "default-case": "off",
        "no-cond-assign": "off",
        "strict": "off",
        "no-loop-func": "off",
        "no-throw-literal": "off",
        "eqeqeq": "off",
        "getter-return": "off",
        "valid-typeof": "off",
        "no-control-regex": "off",
        "no-new-func": "off",
        "no-sparse-arrays": "off",
        "no-fallthrough": "off",
        "no-labels": "off",
        "no-label-var": "off",
        "no-extra-label": "off"
    }
}
