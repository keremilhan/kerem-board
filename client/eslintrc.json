{
    "extends": ["react-app", "plugin:@typescript-eslint/recommended"],
    "plugins": ["@typescript-eslint"],
    "overrides": [
        {
            "files": ["**/*.test.js", "**/*.test.ts", "**/*.test.tsx"],
            "env": {
                "jest": true
            }
        }
    ],
    "rules": { "no-unused-vars": "off", "@typescript-eslint/no-unused-vars": "off", "react-hooks/exhaustive-deps": "off" }
}
